
@{
    Layout = null;
//    var images = ViewBag.imgs;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width"/>
    <title>Apks</title>
    <link href="~/APK.ico" rel="icon" type="image/x-icon"  />
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap/js")
    @Scripts.Render("~/Content/bootstrap-table/bootstrap-table.js")
    @Scripts.Render("~/Content/bootstrap-fileinput/js/fileinput.min.js")
    @Scripts.Render("~/Content/layer/layer.js")
    @Scripts.Render("~/Home/Index")
    @Styles.Render("~/Content/bootstrap/css/bootstrap.css")
    @Styles.Render("~/Content/bootstrap-fileinput/css/fileinput.min.css")
    @Styles.Render("~/Content/bootstrap-table/bootstrap-table.css")
    @* <link href="~/Content/bootstrap/css/bootstrap.css" rel="stylesheet" /> *@
    @* <link href="~/Content/bootstrap-fileinput/css/fileinput.min.css" rel="stylesheet" /> *@
    @* <link href="~/Content/bootstrap-table/bootstrap-table.css" rel="stylesheet" /> *@



</head>
<body>
<div class="container " style="margin-top: 50px">
    <div class="panel panel-primary">
        <div class="panel-heading">AndroidPackage</div>
        <div class="panel-body">
            <button id="btn_import" type="button" class="btn btn-primary btn-lg">
                <span class="glyphicon glyphicon-import" aria-hidden="true"></span>Browse
            </button>
            <button id="btn_qrCode" class="btn btn-primary btn-lg" type="button"><span class="glyphicon glyphicon-qrcode"></span> Generate</button>
        </div>
        <table class="table" id="tb_apkInfos" style="padding:5px">               
        </table>
       
        <form>
            <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <h4 class="modal-title" id="myModalLabel">Select Apk</h4>
                        </div>
                        <div class="modal-body">
                            <input type="file" name="txt_file" id="txt_file" multiple class="file-loading" />
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>
    

</div>


